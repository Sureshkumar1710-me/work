speaker_0_data = global_data[global_data[:, 4] == 0]  # Assuming Speaker column is at index 4
speaker_1_data = global_data[global_data[:, 4] == 1]  # Assuming Speaker column is at index 4

# Example usage or further processing
print("Speaker 0 data:")
print(speaker_0_data)

print("\nSpeaker 1 data:")
print(speaker_1_data)
